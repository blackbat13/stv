		Environment.cardN1=NAceDiamond and Environment.currentPlayer=2 and Environment.clock<4 and (Environment.suit=None or Environment.suit=Diamond or ((Environment.hasSpade<=0 and Environment.suit=Spade) or (Environment.hasClub<=0 and Environment.suit=Club) or (Environment.hasDiamond<=0 and Environment.suit=Diamond) or (Environment.hasHeart<=0 and Environment.suit=Heart))): {PlayAceDiamond};
